{
  "$schema": "nah.nak.pack.v2",
  "nak": {
    "id": "@NAK_ID@",
    "version": "@NAK_VERSION@"
  },
  "paths": {
    "resource_root": "resources",
    "lib_dirs": ["lib"]
  },
  "environment": {
    "NAK_FRAMEWORK_VERSION": "@NAK_VERSION@"
  },
  "loaders": {
    "default": {
      "exec_path": "bin/framework_loader",
      "args_template": ["--app", "{NAH_APP_ENTRY}"]
    }
  },
  "execution": {
    "cwd": "{NAH_APP_ROOT}"
  }
}
